class __DL_Object {
    protected int __dl_ref;

    void init() {
        // after moving to a temperate register it will increase by 1
        self.__dl_ref = 0;
    }

    void __dl_incref() {
        self.__dl_ref++;
    }

    void __dl_decref() {
        if (--self.__dl_ref <= 0) {
            __dlib_free(self);
        }
    }
}
